# Peso Comprimento, max e min ####
TabParametros <- matrix(NA, nrow=length(Comp), ncol=17)
colnames(TabParametros) <- c("CP.min", "CP.max", "WT.min", "WT.max","a","b","2,5%","97,5%","% fêmeas","% machos", "% jovens","% adultos","N total", "N machos", "N fêmeas", "N jovens", "N adultos")
rownames(TabParametros) <- Comp
i=1
for(i in 1:length(Comp)){
  tryCatch({
    TabParametros[i, 1:2] <- range(BioAnual23$CP[BioAnual23$Espécie == Comp[i]], na.rm = TRUE)
    TabParametros[i, 3:4] <- range(BioAnual23$Peso.total..g.[BioAnual23$Espécie == Comp[i]], na.rm = TRUE)
    
    x <- BioAnual23$CP[BioAnual23$Espécie == Comp[i]]
    y <- BioAnual23$Peso.total..g.[BioAnual23$Espécie == Comp[i]]
    
    gender_counts <- BioAnual23$Sexo[BioAnual23$Espécie == Comp[i]]
    gender_counts <- na.omit(gender_counts)
    TabParametros[i, 9] <- (sum(gender_counts == "F", na.rm = TRUE) / length(gender_counts)) * 100
    TabParametros[i, 10] <- (sum(gender_counts == "M", na.rm = TRUE) / length(gender_counts)) * 100
    
    TabParametros[i, 15] <- sum(gender_counts == "F", na.rm = TRUE)
    TabParametros[i, 14] <- sum(gender_counts == "M", na.rm = TRUE)
    
    maturidade <- BioAnual23$Estagio[BioAnual23$Espécie == Comp[i]]
    
    TabParametros[i, 13] <- length(maturidade)
    
    maturidade2 <- maturidade[!is.na(maturidade)]
    
    TabParametros[i, 11] <- (sum(maturidade == "IMT", na.rm = TRUE) / length(maturidade2)) * 100
    
    count <- maturidade[!is.na(maturidade) & maturidade != "IMT"]
    
    TabParametros[i, 12] <- (length(count) / length(maturidade2)) * 100
    
    TabParametros[i, 16] <- sum(maturidade == "IMT", na.rm = TRUE)
    
    TabParametros[i, 17] <- sum(maturidade != "IMT", na.rm = TRUE)
    
    if(length(y) > 20){
      WT_LT <- lm(log10(y) ~ log10(x))
      TabParametros[i, 5] <- round(10^WT_LT$coefficients[1], 4)
      TabParametros[i, 6] <- round(WT_LT$coefficients[2], 4)
      Resumo=summary(WT_LT)
      EP=Resumo$coefficients[2,2]
      parametros=cbind(Resumo$coefficients[,1],confint(WT_LT))
      TabParametros[i, 7] <- parametros[2,2]
      TabParametros[i, 8] <- parametros[2,3]
    }
    
    
  }, error = function(e) {
    cat("Error occurred for species:", Comp[i], "\n")
    TabParametros[i, ] <- NA
  })
}

TabParametros
TabParametros[!is.finite(TabParametros)] <- NA
