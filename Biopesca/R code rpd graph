# ---------------------------------------------
# Preparação do dataset (Mês/Ano + Estágio)
# ---------------------------------------------
month_order <- c("Mar 2025", "Apr 2025", "May 2025", "Jun 2025",
                 "Jul 2025", "Aug 2025", "Sep 2025", "Oct 2025")

rep_data <- BioAnual23 %>%
  mutate(
    Data = as.Date(Data),
    Mes  = format(Data, "%b %Y")
  ) %>%
  filter(!is.na(Estagio)) %>%
  mutate(Mes = factor(Mes, levels = month_order)) %>%
  filter(!is.na(Mes))  # fica só Mar–Out/2025

# ------------------------------------------------
# Ordem e níveis dos estágios (IMPORTANTE!)
# baixo -> topo: IMT, MAT, RPD, ESG, REG
# ------------------------------------------------
stage_levels <- c("IMT", "MAT", "RPD", "ESG", "REG")
rep_data$Estagio <- factor(rep_data$Estagio, levels = stage_levels)

# ---------------------------------------------
# FUNÇÃO: tabela de % por Mês x Estágio
# ---------------------------------------------
make_month_stage_percent <- function(df) {
  df %>%
    filter(!is.na(Estagio), !is.na(Mes)) %>%
    count(Mes, Estagio, name = "n") %>%
    group_by(Mes) %>%
    mutate(pct = 100 * n / sum(n)) %>%
    ungroup() %>%
    complete(
      Mes = factor(month_order, levels = month_order),
      Estagio = factor(stage_levels, levels = stage_levels),
      fill = list(n = 0, pct = 0)
    ) %>%
    arrange(Mes, Estagio)
}

# ---------------------------------------------
# FUNÇÃO: padroniza nome no título (Gênero Espécie; gênero com inicial maiúscula, epíteto minúsculo)
# Ex.: "PROCHILODUS LINEATUS" -> "Prochilodus lineatus"
#      "Prochilodus lineatus" -> "Prochilodus lineatus"
#      "Prochilodus sp.1" -> "Prochilodus sp.1" (mantém o restante como está, mas ajusta gênero)
# ---------------------------------------------
species_title_full <- function(sp_name) {
  parts <- strsplit(trimws(sp_name), "\\s+")[[1]]
  if (length(parts) >= 2) {
    genus <- parts[1]
    rest  <- paste(parts[-1], collapse = " ")
    genus <- paste0(toupper(substr(genus, 1, 1)), tolower(substr(genus, 2, nchar(genus))))
    # mantém o restante como fornecido, mas força minúsculo no 2º termo quando for epíteto simples
    # (se for "sp." ou "sp.1" etc., não força)
    rest_parts <- strsplit(rest, "\\s+")[[1]]
    if (length(rest_parts) >= 1) {
      if (!grepl("^sp\\.?\\d*$", rest_parts[1], ignore.case = TRUE)) {
        rest_parts[1] <- tolower(rest_parts[1])
      }
      rest <- paste(rest_parts, collapse = " ")
    }
    sp_fmt <- paste(genus, rest)
  } else {
    # fallback
    genus <- parts[1]
    genus <- paste0(toupper(substr(genus, 1, 1)), tolower(substr(genus, 2, nchar(genus))))
    sp_fmt <- genus
  }
  
  paste0("Estágios reprodutivos por mês - ", sp_fmt)
}

# ---------------------------------------------
# FUNÇÃO: gráfico empilhado (fundo branco, sem grade)
# ---------------------------------------------
plot_month_stage_stack <- function(tab, titulo = "", italic_title = FALSE) {
  ggplot(tab, aes(x = Mes, y = pct, fill = Estagio)) +
    geom_col(position = "stack", width = 0.8) +
    labs(
      title = titulo,
      x = NULL,  # remove o nome abaixo do eixo X
      y = "Percentual de registros (%)",
      fill = "Estágio"
    ) +
    theme_minimal() +
    theme(
      # remove TODAS as linhas de grade
      panel.grid.major = element_blank(),
      panel.grid.minor = element_blank(),
      
      axis.line.x = element_line(color = "black", linewidth = 1),
      axis.line.y = element_line(color = "black", linewidth = 1),
      
      axis.text.x = element_text(size = 14),
      axis.text.y = element_text(size = 14),
      
      axis.title.y = element_text(size = 16),
      
      # título maior
      plot.title = element_text(
        size = 24,
        face = ifelse(italic_title, "italic", "plain")
      ),
      
      legend.text = element_text(size = 12),
      legend.title = element_text(size = 13),
      legend.position = "right",
      
      # fundo branco (plot + painel)
      plot.background  = element_rect(fill = "white", color = NA),
      panel.background = element_rect(fill = "white", color = NA)
    ) +
    # garante ordem do stack e legenda: IMT MAT RPD ESG REG
    scale_fill_discrete(limits = stage_levels, breaks = stage_levels) +
    guides(fill = guide_legend(reverse = FALSE)) +
    scale_y_continuous(limits = c(0, 100), breaks = seq(0, 100, 10))
}

# ============================================================
# 1) GERAL (todas as espécies) — GRÁFICO
# ============================================================
tab_geral <- make_month_stage_percent(rep_data)

p_geral <- plot_month_stage_stack(
  tab_geral,
  titulo = "Estágios reprodutivos por mês - Geral",
  italic_title = FALSE
)

ggsave(
  filename = file.path(out_dir, "Reprodutivos_por_mes_GERAL.png"),
  plot = p_geral,
  width = 12, height = 7, dpi = 300,
  bg = "white"
)

# ============================================================
# 2) POR ZONA (Lacustre, Transição, Fluvial) — GRÁFICOS
# ============================================================
zonas <- c("Lacustre", "Transição", "Fluvial")

for (z in zonas) {
  
  df_z <- rep_data %>% filter(Zona == z)
  
  tab_z <- make_month_stage_percent(df_z)
  
  p_z <- plot_month_stage_stack(
    tab_z,
    titulo = paste0("Estágios reprodutivos por mês - ", z),
    italic_title = FALSE
  )
  
  # nome seguro p/ arquivo
  z_safe <- iconv(z, to = "ASCII//TRANSLIT")
  if (is.na(z_safe)) z_safe <- z
  z_safe <- gsub("[^A-Za-z0-9_ -]+", "", z_safe)
  z_safe <- gsub(" +", "_", z_safe)
  z_safe <- gsub("_+", "_", z_safe)
  z_safe <- gsub("^_|_$", "", z_safe)
  
  ggsave(
    filename = file.path(out_dir, paste0("Reprodutivos_por_mes_", toupper(z_safe), ".png")),
    plot = p_z,
    width = 12, height = 7, dpi = 300,
    bg = "white"
  )
}

# ============================================================
# 3) POR ESPÉCIE (apenas GRÁFICO por espécie)
# título: itálico, com NOME COMPLETO "Gênero espécie"
# ============================================================
species_list <- sort(unique(rep_data$Espécie))

for (sp in species_list) {
  
  df_sp <- rep_data %>% filter(Espécie == sp)
  
  tab_sp <- make_month_stage_percent(df_sp)
  
  # nome seguro p/ arquivo
  sp_safe <- iconv(sp, to = "ASCII//TRANSLIT")
  if (is.na(sp_safe)) sp_safe <- sp
  sp_safe <- gsub("[^A-Za-z0-9_ -]+", "", sp_safe)
  sp_safe <- gsub(" +", "_", sp_safe)
  sp_safe <- gsub("_+", "_", sp_safe)
  sp_safe <- gsub("^_|_$", "", sp_safe)
  if (nchar(sp_safe) == 0) sp_safe <- "Especie_sem_nome"
  
  p_sp <- plot_month_stage_stack(
    tab_sp,
    titulo = species_title_full(sp),
    italic_title = TRUE
  )
  
  ggsave(
    filename = file.path(out_dir, paste0("Reprodutivos_por_mes_", sp_safe, ".png")),
    plot = p_sp,
    width = 12, height = 7, dpi = 300,
    bg = "white"
  )
}


# Alimentação #####
aliG <- table(BioAnual23$GR)
aliF <- table(BioAnual23$GR[BioAnual23$Zona=="Fluvial"])
aliT <- table(BioAnual23$GR[BioAnual23$Zona=="Transição"])
aliL <- table(BioAnual23$GR[BioAnual23$Zona=="Lacustre"])
aliMat<-t(as.matrix(rbind(aliL,aliT,aliF,aliG)))
aliMat<-aliMat[-5,]
colnames(aliMat) <- c("Lacustre", "Transição", "Fluvial", "Geral")
round(decostand(aliMat, method="total", MARGIN = 2)*100,2)
#jpeg(filename = "Res6_Aliment.jpeg",
#     width = 18, height = 13, units = "cm", pointsize = 12,
#     quality = 100, res=300)
par(mfrow=c(1,1), mar=c(4,6,1,1), oma=c(0,0,0,0))
x1 <- barplot(aliMat, beside=TRUE, horiz=TRUE, yaxt="n", 
              col=terrain.colors(10)[c(4:1)], xlim=c(0, 1.2 * max(aliMat)))
mtext(side=2, line=2, at=colMeans(x1), text = colnames(aliMat), cex=1.25,las=3)
mtext(side=2, line=4, at=mean(x1), text = "Grau de Repleção", cex=1.25,las=3)
axis(side=2, at=x1, labels=rep(rownames(aliMat),4), cex=0.7,las=2)
mtext("Frequência (Número de indivíduos)", side=1, line=3, cex=1.25)
dev.off()

# Considerações finais
levels(factor(BioAnual23$Mes)) # conferir meses no periodo
nrow(BioAnual23) # numero de exemplares

captura.peso<-tapply(BioAnual23$Peso.total..g., BioAnual23$Espécie, sum, na.rm=T)
sort(round(captura.peso/sum(captura.peso)*100,1)) # contribuição na peso total

captura.numero<-tapply(BioAnual23$Peso.total..g., BioAnual23$Espécie, length)
sort(round(captura.numero/sum(captura.numero)*100,1)) # contribuição no numero total

