## COLOQUEI 0 no lugar de NaN pois tinha valores ausentes
J_long <- J_long %>% 
  mutate(across(where(is.numeric), ~ ifelse(is.nan(.), 0, .)))

#tirei o zero do gráfico pra ocultar o problema da equitabilidade
J_long <- J_long %>% filter(Value != 0)

